/**
 * @param num: a positive number
 * @return: true if it's a palindrome or false
 */
const isPalindrome = function (num) {
    var str = num.toString();
    var reverse = "";
    for (var i = str.length - 1; i >= 0; i--){
        reverse += str[i];
    }
    if (reverse === str){
        return true;
    } else {
        return false;
    }
}
