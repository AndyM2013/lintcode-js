/**
 * @param n: An integer
 * @return: A list of strings.
 */
const fizzBuzz = function (n) {
    var arr = [];
    for (var i = 1; i <= n; i++){
        if (i % 15 === 0){
            arr.push("fizz buzz");
        } else if (i % 5 === 0){
            arr.push("buzz");
        } else if (i % 3 === 0){
            arr.push("fizz");
        } else {
            arr.push(i.toString());
        }
    }
    return arr;
}

