/**
 * @param a: An integer
 * @param operator: A character, +, -, *, /.
 * @param b: An integer
 * @return: The result
 */
const calculate = function (a, operator, b) {
    // write your code here
    switch(operator){
        case'+': return a + b;
        case'-': return a - b;
        case'*': return a * b;
        case'/': return a / b;
    }
    return 0;
}
